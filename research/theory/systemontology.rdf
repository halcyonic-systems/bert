        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Energy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#emergesFrom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#emergesFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#existsInEnvironment -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#existsInEnvironment">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Environment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasBoundary -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasBoundary">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Boundary"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasComponent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasComponent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasInput -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasInput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasOutput -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasOutput">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>

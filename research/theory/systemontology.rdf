    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSink -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSink">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Output"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Sink"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSource -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSource">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Source"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSubsystem -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasSubsystem">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Subsystem"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#interactsWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#interactsWith">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Output"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#isPartOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Subsystem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#transformsFundamentalElement -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#transformsFundamentalElement">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:ObjectProperty>
    



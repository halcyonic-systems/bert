    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#HasBoundaryPermeability -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#HasBoundaryPermeability">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasComplexity -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasComplexity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasHierarchyLevel -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#hasHierarchyLevel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#AmplifyingProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AmplifyingProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>An atomic work process that increases the magnitude of a signal or flow, often using external energy.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component"/>
        <rdfs:comment>A fundamental, indivisible unit of work within a system that transforms inputs into outputs by manipulating the fundamental elements of Matter, Energy, Information, and Knowledge. These processes form the basis of all system operations.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Boundary -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Boundary">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#emergesFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Definition: The conceptual or physical delineation between a system and its environment, emerging from specific configurations of fundamental elements. It controls the flow of Information, Knowledge, Matter, and Energy between the system and its surroundings.

Example: A cell membrane acts as a boundary, selectively allowing nutrients to enter and waste to exit the cell.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#BufferingProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#BufferingProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>A process that temporarily stores a substance to regulate its flow.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#CombiningProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#CombiningProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>A process that combines two or more inputs into a single output</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#ComplexAdaptiveSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#ComplexAdaptiveSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#System"/>
        <rdfs:comment>A system composed of many interacting parts that has the ability to change and learn from experience, often exhibiting non-linear behavior and emergent properties. It dynamically reconfigures its internal relationships of Matter, Energy, Information, and Knowledge in response to environmental changes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Component">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#emergesFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Definition: A fundamental part of a system that emerges from configurations of Information, Knowledge, Matter, and Energy. It performs specific functions and interacts with other components to create the system&apos;s behavior.

Example: In a computer system, the CPU is a component that processes instructions, interacting with memory and other components to perform computations.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#CopyingProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#CopyingProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>An atomic work process that replicates an input to produce identical or similar outputs, often used in information processing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Disturbance -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Disturbance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Entities"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Energy -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Energy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#EnergyInput -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#EnergyInput">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#EnergyOutput -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#EnergyOutput">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Output"/>
        <rdfs:comment>Definition: Matter, energy, or information that leaves a system and enters its environment as a result of the system&apos;s processes.

Example: In a manufacturing system, finished products are outputs that enter the market environment.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Entities -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Entities">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Environment"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Environment -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Environment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#emergesFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Definition: The context or surroundings in which a system exists, emerging from complex interactions of Information, Knowledge, Matter, and Energy. It includes all factors and conditions external to the system that may influence its behavior and development..

Example: For a business system, the environment includes the market conditions, regulatory framework, and technological landscape in which it operates.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Flows -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Flows">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Environment"/>
        <rdfs:comment>Definition: The movement of matter, energy, or information through a system or between a system and its environment.

Example: In an electrical grid, the transmission of electricity from power plants to consumers represents a flow of energy.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#ImpedingProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#ImpedingProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>A process that slows or resists the flow of a substance.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Information -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Information">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Flows"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#consistsOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Output"/>
        <rdfs:comment>Definition: Matter, Energy, Information, or Knowledge that enters a system from its environment. Inputs consist of one or more fundamental elements and are used by the system in its processes.

Example: Solar radiation is an input for photosynthetic plants, used to drive the process of photosynthesis.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#InvertingProcess -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#InvertingProcess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#AtomicWorkProcess"/>
        <rdfs:comment>An atomic work process that reverses or negates the characteristics of an input, often used in control systems or signal processing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Knowledge -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Knowledge">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#MaterialInput -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#MaterialInput">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Input"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#MaterialOutput -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#MaterialOutput">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Output"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#Matter -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#Matter">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/shingai/ontologies/2024/6/system#FundamentalElement"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#MessageInput -->

    <owl:Class rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#MessageInput">

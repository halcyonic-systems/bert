     -->

    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#adaptsTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#adaptsTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#consistsOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#consistsOf"/>
    


    <!-- http://www.semanticweb.org/shingai/ontologies/2024/6/system#consumesEnergy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/shingai/ontologies/2024/6/system#consumesEnergy">
